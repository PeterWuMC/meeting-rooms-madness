%table.table
  %tr
    %th Room
    %th From
    %th To
    %th
  - @rooms.each do |room, time_hash|
    %tr
      %td= room.name.humanize
      %td= l(time_hash[:start])
      %td= l(time_hash[:end])
      %td= link_to 'Book it', room_events_path(room.name, duration: 1.hour, from_time: time_hash[:start], to_time: time_hash[:end]), :method=> :post
